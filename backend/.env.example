# Environment Configuration for EIDO Backend

# Application
DEBUG=false
ENVIRONMENT=development
LOG_LEVEL=INFO

# Database
DATABASE_URL=sqlite:///./eido.db

# CORS
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8000

# External Services - SURGE (Tokenization)
SURGE_API_KEY=your_surge_api_key_here
SURGE_TESTNET=true

# External Services - Moltbook (Publishing)
MOLTBOOK_API_KEY=your_moltbook_api_key_here

# External Services - here.now (Deployment)
HERENOW_API_KEY=your_herenow_api_key_here

# Agent Configuration
MAX_AGENT_RETRIES=3
AGENT_TIMEOUT_SECONDS=300
